<script type="text/javascript"> 
function clickFunction(url,mc){
        	  if(!JsTjdlXz('QYSDSXZ')){
        	         return false;
        		 }
              window.parent.showPage4(url,mc);
              
          }
var jdSkssq = [["-01-01","-03-31"],["-04-01","-06-30"],["-07-01","-09-30"],["-10-01","-12-31"]];
function initSubmit(flag){
	var skssqq = $("#skssqq").val();
	var skssqz = $("#skssqz").val();
	if (skssqq == "" || skssqq * 1 < 999) {
		layer.alert("请输入合法的年度！",{icon:5});
		$("#skssqq").focus();
		return false;
	}
	if (skssqz == "" || skssqz * 1 > 4 || skssqz * 1 < 1) {
		layer.alert("请输入合法的季度！",{icon:5});
		$("#skssqz").focus();
		return false;
	 }
	layer.load(1,{shade:[0.4 , '#FFF' , true]});
	skssqq = skssqq + jdSkssq[skssqz-1][0];
	skssqz = $("#skssqq").val() + jdSkssq[skssqz-1][1];
	$("#sbbVO\\[\\'SKSSQQ\\'\\]").val(skssqq);
	$("#sbbVO\\[\\'SKSSQZ\\'\\]").val(skssqz);
	if(flag == 'jysb'){
		$("#initForm").attr('action','/jsdzsb/qydsdjd/czzsJysbInCheck.do');
	}else if(flag == 'yjlsb'){
		$("#initForm").attr('action','/jsdzsb/qydsdjd/czzsYjlsbInCheck.do');
	}else if(flag == 'gzsb'){
		$("#initForm").attr('action','/jsdzsb/qydsdjd/czzsGzsbInCheck.do');
		$("#initForm").submit();
		return true;
	}else{
		$("#initForm").attr('action','/jsdzsb/qydsdjd/czzsInCheck.do');
	}
	if (JsYsqbChecker(skssqq,skssqz,"10104","","08","")) {
		layer.load(1,{shade:[0.4 , '#FFF' , true]});
		setTimeout(function(){$("#initForm").submit();},300);
	} else {
  		layer.closeAll('loading');
	} 
}
</script>


		

