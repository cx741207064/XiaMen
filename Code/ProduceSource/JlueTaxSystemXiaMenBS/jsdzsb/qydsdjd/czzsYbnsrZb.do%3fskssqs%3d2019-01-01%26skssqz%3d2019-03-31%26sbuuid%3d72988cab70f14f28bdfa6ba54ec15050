/**把返回的自动计算值写到表单各字段*/
function setAutoCalcValues(data, param, flag){
  DWRUtil.setValue("sbbVO['LJ_XSYYJE']", add_Qfw(toFixedNum(data.LJ_XSYYJE,2)));
  DWRUtil.setValue("sbbVO['LJ_YYCB']", add_Qfw(toFixedNum(data.LJ_YYCB,2)));
  DWRUtil.setValue("sbbVO['LJ_LRZE']", add_Qfw(toFixedNum(data.LJ_LRZE,2)));
  DWRUtil.setValue("sbbVO['SNDYNSSDE']", add_Qfw(toFixedNum(data.SNDYNSSDE,2)));
  DWRUtil.setValue("sbbVO['LJ_YNSSDE']", add_Qfw(toFixedNum(data.LJ_YNSSDE,2)));
  DWRUtil.setValue("sbbVO['LJ_SYSL']", data.LJ_SYSL);
  DWRUtil.setValue("sbbVO['BQ_YHSL']", data.BQ_YHSL);
  DWRUtil.setValue("sbbVO['LJ_YHSL']", data.LJ_YHSL);
  DWRUtil.setValue("sbbVO['BQ_JZL']", data.BQ_JZL);
  DWRUtil.setValue("sbbVO['LJ_JZL']", data.LJ_JZL);
   DWRUtil.setValue("sbbVO['YSBJDS']", data.YSBJDS);
  DWRUtil.setValue("sbbVO['LJ_YNSDSE']", add_Qfw(toFixedNum(data.LJ_YNSDSE,2)));
  DWRUtil.setValue("sbbVO['LJ_SJYNSDSE']", add_Qfw(toFixedNum(data.LJ_SJYNSDSE,2)));
  DWRUtil.setValue("sbbVO['LJ_YHSLJZE']", add_Qfw(toFixedNum(data.LJ_YHSLJZE,2)));
  DWRUtil.setValue("sbbVO['LJ_JMSDSE']", add_Qfw(toFixedNum(data.LJ_JMSDSE,2)));
 
  DWRUtil.setValue("sbbVO['LJ_SJYJNSDSE']", add_Qfw(toFixedNum(data.LJ_SJYJNSDSE,2)));
  DWRUtil.setValue("sbbVO['LJ_YBTSDSE']", add_Qfw(toFixedNum(data.LJ_YBTSDSE,2)));
  if(param.LJ_JMSDSE=="") DWRUtil.setValue("sbbVO['LJ_JMSDSE']", "");
  
  DWRUtil.setValue("sbbVO['LJ_LRZE_TZQ']", add_Qfw(toFixedNum(data.LJ_LRZE_TZQ,2)));
  DWRUtil.setValue("sbbVO['LJ_TDYWJSYNSSDE']", add_Qfw(toFixedNum(data.LJ_TDYWJSYNSSDE,2)));
  DWRUtil.setValue("sbbVO['LJ_BZSSR']", add_Qfw(toFixedNum(data.LJ_BZSSR,2)));
  DWRUtil.setValue("sbbVO['LJ_MSSRJSSRSDJMDYHJE']", add_Qfw(toFixedNum(data.LJ_MSSRJSSRSDJMDYHJE,2)));
  DWRUtil.setValue("sbbVO['LJ_GDZCJSZJTJE']", add_Qfw(toFixedNum(data.LJ_GDZCJSZJTJE,2)));
 
  DWRUtil.setValue("sbbVO['LJ_JMSDSE']", add_Qfw(toFixedNum(data.LJ_JMSDSE),2));  
  
  DWRUtil.setValue("sbbVO['LJ_NBYQNDKS']", add_Qfw(toFixedNum(data.LJ_NBYQNDKS,2)));
  DWRUtil.setValue("sbbVO['LJ_TDYWYJSDSE']", add_Qfw(toFixedNum(data.LJ_TDYWYJSDSE,2)));
  DWRUtil.setValue("sbbVO['LJ_YBTSDSE_TZQ']", add_Qfw(toFixedNum(data.LJ_YBTSDSE_TZQ,2)));
  DWRUtil.setValue("sbbVO['LJ_YQNDDJBQDJSDSE']", add_Qfw(toFixedNum(data.LJ_YQNDDJBQDJSDSE,2)));
  DWRUtil.setValue("sbbVO['JCCYRS']", data.JCCYRS);
  DWRUtil.setValue("sbbVO['JMCYRS']", data.JMCYRS);
  DWRUtil.setValue("sbbVO['JCZZZE']", add_Qfw(toFixedNum(data.JCZZZE,2)));
  DWRUtil.setValue("sbbVO['JMZZZE']", add_Qfw(toFixedNum(data.JMZZZE,2)));
  if(!false){
	  $('#_SFXWQY_BZ').val(data.SFXWQY_BZ);
	  DWRUtil.setValue("sbbVO['SFXWQY_BZ']", data.SFXWQY_BZ);
  }
  BQ_B3_N2=data.BQ_B3_N2;
  BQ_B3_N3=data.BQ_B3_N3;
  LJ_B3_N2=data.LJ_B3_N2;
  LJ_B3_N3=data.LJ_B3_N3;
  
}

/**营业收入改变事件*/
function onBQ_XSYYJEChange(){
  return true;
}

/**利润总额改变事件*/
function onLJ_LRZEChange(flag){
  var param = getFormData();
  param.SYZE="YES";

  var LJ_LRZE =getoff_Qfw(DWRUtil.getValue("sbbVO['LJ_LRZE']"));
  var LJ_JMSDSE=getoff_Qfw(DWRUtil.getValue("sbbVO['LJ_JMSDSE']"));//13
  var LJ_XXWLQYJMSDSE=getoff_Qfw(DWRUtil.getValue("sbbVO['LJ_XXWLQYJMSDSE']"));//14
  if(LJ_LRZE<=1000000){
	if(LJ_XXWLQYJMSDSE>LJ_JMSDSE){
		alert("当“实际利润额”≤100万时,第13行必须大于等于第14行。");
		DWRUtil.focusByIds("sbbVO['LJ_JMSDSE']");
		return false;
	}
  }
  jdsbCalcForYbnsr(param,flag);
}
function onLJ_LRZEChange2(){
  var requestId = "";
  var param = getFormData();
  param.SYZE="NO";
  var LJ_LRZE =getoff_Qfw(DWRUtil.getValue("sbbVO['LJ_LRZE']"));
  var LJ_JMSDSE=getoff_Qfw(DWRUtil.getValue("sbbVO['LJ_JMSDSE']"));//13
  var LJ_XXWLQYJMSDSE=getoff_Qfw(DWRUtil.getValue("sbbVO['LJ_XXWLQYJMSDSE']"));//14
  if(LJ_LRZE<=500000){
	if(LJ_XXWLQYJMSDSE>LJ_JMSDSE){
	  alert("当“实际利润额”≤50万时,第13行必须大于等于第14行。");
		DWRUtil.focusByIds("sbbVO['LJ_JMSDSE']");
	  return false;
	}
  }
  jdsbCalcForYbnsr(param);
}

function onSFGJXZHJZHYChange(){
	var SFGJXZHJZHY =DWRUtil.getValue("sbbVO['SFGJXZHJZHY']");
	if(SFGJXZHJZHY == 'Y'){
		parent.layer.confirm('请确认是否为国家限制或禁止行业'
				, {btn: ['是','否'],icon: 3, title:'提示'}, function(index){
					parent.layer.close(index);
					onLJ_LRZEChange();
		},function(index){
			DWRUtil.focusByIds("sbbVO['SFGJXZHJZHY']");
			DWRUtil.setValue("sbbVO['SFGJXZHJZHY']","N");
		});
	}else{
		onLJ_LRZEChange();
	}
}

function onCYRSAndZCZEChange(flag){
	var jccyrs = getoff_Qfw(DWRUtil.getValue("sbbVO['JCCYRS']"));
	var jmcyrs = getoff_Qfw(DWRUtil.getValue("sbbVO['JMCYRS']"));
	var jczzze = getoff_Qfw(DWRUtil.getValue("sbbVO['JCZZZE']"));
	var jmzzze = getoff_Qfw(DWRUtil.getValue("sbbVO['JMZZZE']"));
	if(flag == '1' && parseFloat(jccyrs) == 0){
		parent.layer.confirm('请确认季初从业人数为0'
				, {icon: 3, title:'提示'}, function(index){
					parent.layer.close(index);
					onLJ_LRZEChange();
		},function(index){
			DWRUtil.focusByIds("sbbVO['JCCYRS']");
			DWRUtil.setValue("sbbVO['JCCYRS']","");
		});
	}else if(flag == '2' && parseFloat(jmcyrs) == 0){
		parent.layer.confirm('请确认季末从业人数为0'
				, {icon: 3, title:'提示'}, function(index){
					parent.layer.close(index);
					onLJ_LRZEChange();
		},function(index){
			DWRUtil.focusByIds("sbbVO['JMCYRS']");
			DWRUtil.setValue("sbbVO['JMCYRS']","");
		});
	}else if(flag == '3' && parseFloat(jczzze) == 0){
		parent.layer.confirm('请确认季初资产总额为0'
				, {icon: 3, title:'提示'}, function(index){
					parent.layer.close(index);
					onLJ_LRZEChange();
		},function(index){
			DWRUtil.focusByIds("sbbVO['JCZZZE']");
			DWRUtil.setValue("sbbVO['JCZZZE']","");
		});
	}else if(flag == '3' && parseFloat(jczzze) > 5000){
		parent.layer.alert('资产总额是判断小型微利企业的的重要条件，单位为“万元”，请准确填报');
		onLJ_LRZEChange();
	}else if(flag == '4' && parseFloat(jmzzze) == 0){
		parent.layer.confirm('请确认季末资产总额为0'
				, {icon: 3, title:'提示'}, function(index){
					parent.layer.close(index);
					onLJ_LRZEChange();
		},function(index){
			DWRUtil.focusByIds("sbbVO['JMZZZE']");
			DWRUtil.setValue("sbbVO['JMZZZE']","");
		});
	}else if(flag == '4' && parseFloat(jmzzze) > 5000){
		parent.layer.alert('资产总额是判断小型微利企业的的重要条件，单位为“万元”，请准确填报');
		onLJ_LRZEChange();
	}else{
		onLJ_LRZEChange();
	}
}

function jdsbCalcForYbnsr(param,flag) {
  layer.load(1,{shade:[0.4 , '#FFF' , true]});
  var postData = {};
  postData["sbbVO"] = param;
  $.ajax({
	url: "/jsdzsb/qydsdjd/czzsYbnsrCalc.do",
	data: $.toJSON(postData),
	type: 'post',
	cache: false,
	contentType:'application/json;charset=UTF-8',
	success : function(data) {
	  if (data.success) {
		  setAutoCalcValues(data, param,flag);
	  } else {
		layer.alert(data.msg,{icon:5});
	  }
	},
	complete: function() {
	  layer.closeAll('loading');
	}
  });
}

