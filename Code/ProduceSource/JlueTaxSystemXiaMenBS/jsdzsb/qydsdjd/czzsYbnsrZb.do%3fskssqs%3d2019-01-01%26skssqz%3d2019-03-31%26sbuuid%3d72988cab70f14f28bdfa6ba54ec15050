$(function(){  
	endIe8Load();
}); 

function endIe8Load() {
	try{
		var _loadIfm = ' <iframe id="_loadIfm" src="" width="1" height="1" style="border: 0px; display:none" frameborder="0" ></iframe>';
		if (!$("#_loadIfm")[0]) {
			$("#_loadIfmContent").append(_loadIfm);
		}
		$("#_loadIfmContent", window.document).attr("src", "#");
	}catch(e){}
}
