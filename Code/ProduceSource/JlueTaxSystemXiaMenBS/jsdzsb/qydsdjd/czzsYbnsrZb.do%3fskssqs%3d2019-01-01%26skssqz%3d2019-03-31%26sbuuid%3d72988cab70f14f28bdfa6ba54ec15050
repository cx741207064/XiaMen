	onLJ_LRZEChange();	
}
function show2(event){	
	onLJ_LRZEChange();	
}
function show3(evt){	
	onLJ_LRZEChange();	
}
function show4(evt){	
	onLJ_LRZEChange();	
}
function show5(evt){	
	onLJ_LRZEChange();	
}

//将confirm的“确认”“取消”修改成“是”“否”
function confirmYn(str) {   
    execScript("n = (msgbox('"+str+"',vbYesNo, '提示')=vbYes)", "vbscript");  
    return(n);   
}

/**申报按钮事件*/
function onDeclareClick(){
  var param = getFormData();

  var jccyrs = getoff_Qfw(DWRUtil.getValue("sbbVO['JCCYRS']"));
  var jmcyrs = getoff_Qfw(DWRUtil.getValue("sbbVO['JMCYRS']"));
  var jczzze = getoff_Qfw(DWRUtil.getValue("sbbVO['JCZZZE']"));
  var jmzzze = getoff_Qfw(DWRUtil.getValue("sbbVO['JMZZZE']"));
  
  if(parseFloat(jccyrs) < 0 || !/^[0-9]*[1-9][0-9]*$/.test(jccyrs)){
	  alert("季初从业人数必须大于等于0并且是整数。");
	  DWRUtil.focusByIds("sbbVO['JCCYRS']");
	  DWRUtil.setValue("sbbVO['JCCYRS']",0);
	  return false;
  }
  
  if(parseFloat(jmcyrs) <= 0 || !/^[0-9]*[1-9][0-9]*$/.test(jmcyrs)){
	  alert("季末从业人数必须大于等于0并且是整数。");
	  DWRUtil.focusByIds("sbbVO['JMCYRS']");
	  DWRUtil.setValue("sbbVO['JMCYRS']",0);
	  return false;
  }
  
  if(parseFloat(jczzze) < 0){
	  alert("季初资产总额（万元）必须大于等于0.");
	  DWRUtil.focusByIds("sbbVO['JCZZZE']");
	  DWRUtil.setValue("sbbVO['JCZZZE']",0);
	  return false;
  }
  
  if(parseFloat(jmzzze) < 0){
	  alert("季末资产总额（万元）必须大于等于0.");
	  DWRUtil.focusByIds("sbbVO['JMZZZE']");
	  DWRUtil.setValue("sbbVO['JMZZZE']",0);
	  return false;
  }

  
  
  if(param.LJ_XSYYJE=="") {
    alert("营业收入必须录入。");
	DWRUtil.focusByIds("sbbVO['LJ_XSYYJE']");
    return false;
  }
  if(param.LJ_YYCB=="") {
    alert("营业成本必须录入。");
	DWRUtil.focusByIds("sbbVO['LJ_YYCB']");
    return false;
  }
  if(param.LJ_LRZE_TZQ=="") {
    alert("利润总额必须录入。");
	DWRUtil.focusByIds("sbbVO['LJ_LRZE_TZQ']");
    return false;
  }
  if('10' == 20 && param.LJ_LRZE_TZQ <= 0){
	    alert("当“预缴方式”为“按照上一纳税年度应纳税所得额平均额预缴”时,利润总额必须大于0");
		DWRUtil.focusByIds("sbbVO['LJ_LRZE_TZQ']");
	    return false;
  }
  if(parseFloat(param.LJ_TDYWJSYNSSDE) != 0 && '7212' != '7010' && '7212' != '7040'){
	    alert("本表第4行为房地产开发企业填报，贵单位税务登记信息所属行业不为房地产开发经营，不允许填报此行。");
		DWRUtil.focusByIds("sbbVO['LJ_TDYWJSYNSSDE']");
	    return false;
  }
  if(parseFloat(param.LJ_TDYWJSYNSSDE) < 0){
	    alert("加：特定业务计算的应纳税所得额本年累计金额必选大于等于0。");
		DWRUtil.focusByIds("sbbVO['LJ_TDYWJSYNSSDE']");
	    return false;
  }
  if (param.LJ_JMSDSE == "") {
	DWRUtil.setValue("sbbVO['LJ_JMSDSE']",0);
    return false;
  }
  if (parseFloat(param.LJ_JMSDSE) < 0) {
    alert("减免所得税额（累计金额）必须大于等于0。");
	DWRUtil.focusByIds("sbbVO['LJ_JMSDSE']");
    return false;
  }
	if(parseFloat(param.LJ_BZSSR) < 0){
		  alert("减:不征税收入不得小于0。");
		  DWRUtil.focusByIds("sbbVO['LJ_BZSSR']");
		 return false;
	}
	if(parseFloat(param.LJ_GDZCJSZJTJE) < 0){
		  alert("累计    固定资产加速折旧（扣除）调减额不得小于0。");
		  DWRUtil.focusByIds("sbbVO['LJ_GDZCJSZJTJE']");
		 return false;
	}
  if(parseFloat(param.LJ_XXWLQYJMSDSE)>parseFloat(param.LJ_JMSDSE)){
	alert("减免所得税额（累计金额） 必须大于等于 符合条件的小型微利企业减免所得税额   。");
	DWRUtil.focusByIds("sbbVO['LJ_JMSDSE']");
	return false;
  }
	if(parseFloat(param.LJ_TDYWYJSDSE) != toFixedNum(0,2)){
		alert("如需填报本表第14行，请携带相关缴税凭证到办税大厅柜台申报。");
		DWRUtil.setValue("sbbVO['LJ_TDYWYJSDSE']", add_Qfw(toFixedNum(0,2)));
		DWRUtil.focusByIds("sbbVO['LJ_TDYWYJSDSE']");
		return false;
	}
  

	  
  
	  
  

  if(param.LJ_SJYJNSDSE=="") {
	DWRUtil.setValue("sbbVO['LJ_SJYJNSDSE']",0);
  }
  
  //20190408新增小微校验
  var lj_lrze = parseFloat(param.LJ_LRZE);
  var sfgjxzhjzhy = param.SFGJXZHJZHY;
  var sfxwqy_bz = DWRUtil.getValue("sbbVO['SFXWQY_BZ']");
  var CYRSPJSLJS = parseFloat(param.CYRSPJSLJS);
  var ZCZEPJSLJS = parseFloat(param.ZCZEPJSLJS);
  var YSBJDS = parseFloat(param.YSBJDS);
  var SBQYLX = 0;
  var DJZFJGLBDM = 0;
  jccyrs = parseFloat(jccyrs);
  jmcyrs = parseFloat(jmcyrs) 
  jczzze = parseFloat(jczzze);
  jmzzze = parseFloat(jmzzze);
  jczzze = jczzze.toFixed(2);
  jmzzze = jmzzze.toFixed(2);
  var PJRS = 0;
  var PJZCZE = 0;
  var DJPJCYRS = ((jmcyrs+jccyrs)/2).toFixed(2);
  CYRSPJSLJS = CYRSPJSLJS + DJPJCYRS;
  PJRS = Math.round(CYRSPJSLJS/(YSBJDS+1));
  var DJPJZCZE = ((jczzze+jmzzze)/2).toFixed(2);
  ZCZEPJSLJS = ZCZEPJSLJS + DJPJZCZE;
  PJZCZE = (ZCZEPJSLJS/(YSBJDS+1)).toFixed(2); 
  if(sfgjxzhjzhy == 'N' && PJRS <= 300 && PJZCZE <= 5000 && lj_lrze <= 3000000 && sfxwqy_bz == 'N'){
	  if(!(SBQYLX == '0' && (DJZFJGLBDM == '2' || DJZFJGLBDM == '3'))){
		  alert("当本期“国家限制或禁止行业”选择“否”,平均从业人数<=300,平均资产总额(万元)<=5000,应纳税所得额<=3000000时，是否小微企业应选择是");
		  return false;
	  }
  }
  if(SBQYLX == '0' && (DJZFJGLBDM == '2' || DJZFJGLBDM == '3') && sfxwqy_bz == 'Y'){
	  alert("企业类型为一般企业的分支机构是否小微默认否");
	  return false;
  }
  if(!(sfgjxzhjzhy == 'N' && PJRS <= 300 && PJZCZE <= 5000 && lj_lrze <= 3000000) && sfxwqy_bz == 'Y'){
	  alert("小微校验不通过");
	  return false;
  }

  alert("季初资产总额、季末资产总额单位为“万元”，请准确填报。");
  
  layer.load(0,{shade:[0.5 , '#8e8efe' , true]});
  $("#initForm").submit();
	
  return true;
}

$(function(){ 
	layer.alert("尊敬的纳税人，从2018年第二季度起，企业所得税季度预缴纳税申报表(A类)，仅填报<font color='red'>本年累计数</font>，请准确填报。<br/>提示：仅下划横线栏次可以填报");
	
	if('72988cab70f14f28bdfa6ba54ec15050'!=''){
		onLJ_LRZEChange(true);
	}
	
});
function LJ_LRZECHANGE(){
	LJ_LRZECHANGE_BZ="Y";
}

function showTbsm(swwsdm) {
	var url="/res/staticHtml/tbsm/TBSM" + swwsdm + ".html";
	if(window.showModalDialog){
	    var opensWin = window.showModalDialog(url,window,'dialogWidth:950px;dialogHeight:550px;dialogLeft:100px;dialogTop:50px;center:no;status:0;help:0;edge:sunken;scroll:yes');
		if(opensWin!=null && opensWin!=""){
		     window.location=opensWin;
		}	
	}else{
		var title = "填表说明";
		var lnkUrl = "/res/staticHtml/tbsm/TBSM" + swwsdm + ".html";
		layer.open({
			  type: 2,
			  title: title,
			  shadeClose: true,
			  shade: [0.5 , '#8e8efe' , true],
			  area: ['950px', '90%'],
			  content: lnkUrl
		});
	}
}
</script>

		

