/**利润总额改变事件*/
function onLJ_LRZEChange(flag){
	var param = getFormData();
  	jdsbCalcForYbnsr(param,flag);
}

function jdsbCalcForYbnsr(param,flag) {
  layer.load(1,{shade:[0.4 , '#FFF' , true]});
  var postData = {};
  postData["sbbVO"] = param;
  $.ajax({
	url: "/jsdzsb/qydsdjd/czzsYbnsrCalc.do",
	data: $.toJSON(postData),
	type: 'post',
	cache: false,
	contentType:'application/json;charset=UTF-8',
	success : function(data) {
	  if (data.success) {
		  setAutoCalcValues(data, param,flag);
	  } else {
		layer.alert(data.msg,{icon:5});
	  }
	},
	complete: function() {
	  layer.closeAll('loading');
	}
  });
}

